
var view = UILabel()

view.frame = CGRect(x: 0, y: 0, width: 250, height: 269)

view.backgroundColor = .white


let image0 = UIImage(named: "we-removebg-preview.png")?.cgImage

let layer0 = CALayer()

layer0.contents = image0

layer0.transform = CATransform3DMakeAffineTransform(CGAffineTransform(a: 1.08, b: 0, c: 0, d: 1, tx: -0.04, ty: 0))

layer0.bounds = view.bounds

layer0.position = view.center

view.layer.addSublayer(layer0)



var parent = self.view!

parent.addSubview(view)

view.translatesAutoresizingMaskIntoConstraints = false

view.widthAnchor.constraint(equalToConstant: 250).isActive = true

view.heightAnchor.constraint(equalToConstant: 269).isActive = true

view.leadingAnchor.constraint(equalTo: parent.leadingAnchor, constant: 82).isActive = true

view.topAnchor.constraint(equalTo: parent.topAnchor, constant: 14).isActive = true




